      </transition>
      <transition id="T-lb_no_balance">
        <name>
          <text>lb_no_balance</text>
        </name>
        <condition>
          <text>(((l1 = l2) or (l1 = (succ l2))) or (l2 = (succ l1)))</text>
          <structure>
            <or>
              <subterm>
                <or>
                  <subterm>
                    <equality>
                      <subterm>
                        <variable refvariable="V-l1-clients_no"/>
                      </subterm>
                      <subterm>
                        <variable refvariable="V-l2-clients_no"/>
                      </subterm>
                    </equality>
                  </subterm>
                  <subterm>
                    <equality>
                      <subterm>
                        <variable refvariable="V-l1-clients_no"/>
                      </subterm>
                      <subterm>
                        <successor>
                          <subterm>
                            <variable refvariable="V-l2-clients_no"/>
                          </subterm>
                        </successor>
                      </subterm>
                    </equality>
                  </subterm>
                </or>
              </subterm>
              <subterm>
                <equality>
                  <subterm>
                    <variable refvariable="V-l2-clients_no"/>
                  </subterm>
                  <subterm>
                    <successor>
                      <subterm>
                        <variable refvariable="V-l1-clients_no"/>
                      </subterm>
                    </successor>
                  </subterm>
                </equality>
              </subterm>
            </or>
          </structure>
        </condition>
      </transition>
      <transition id="T-lb_balance_to_1">
        <name>
          <text>lb_balance_to_1</text>
        </name>
        <condition>
          <text>(l2 = (succ (succ l1)))</text>
          <structure>
            <equality>
              <subterm>
                <variable refvariable="V-l2-clients_no"/>
              </subterm>
              <subterm>
                <successor>
